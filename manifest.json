{
  "manifest_version": 3,
  "name": "Tab Organizer",
  "version": "0.1.3",
  "description": "Group tabs by domain and export or import open tabs with ease.",
  "permissions": [
    "tabs",
    "tabGroups",
    "downloads"
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
